{
    "manifest_version": 2,
    "name": "Google consent dialogs handler",
    "version": "0.1",
    "description": "Google \"Before you continue\" and \"Sign in to YouTube\" dialogs handler",
    "icons": {
        "48": "icons/light.svg",
        "96": "icons/light.svg"
    },
    "content_scripts": [
        {
            "all_frames": true,
            "matches": [
                 "*://*/*"
            ],
            "js": [ "beforeYouContinueClicker.js" ]
        }
        ,
        {
            "matches": [
                 "*://*.youtube.com/*"
            ],
            "js": [ "signInToYouTubeClicker.js" ]
        }
    ],
    "web_accessible_resources": [ "page-scripts/onLoad.js" ]
}